{
  "name": "cra-template-fleflis-ts",
  "version": "0.1.2",
  "private": false,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "styled-components": "^5.2.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx  --fix ./",
    "clean-files": "rm -rf ./template/public ./template/src ./template/.vscode && rm -f ./template/.editorconfig ./template/.eslintignore ./template/.eslintrc.js ./template/prettier.config.js ./template/README.md ./template/tsconfig.json",
    "copy-files": "cp -a ./src/. ./template/src && cp -a ./public/. ./template/public && cp -a ./.vscode/. ./template/.vscode && cp .eslintignore .eslintrc.js .editorconfig prettier.config.js tsconfig.json template/ && cp README_CRA.md template/README.md",
    "prepublishOnly": "yarn clean-files && yarn copy-files"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "template",
    "template.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fleflis/cra-template-fleflis-ts",
    "directory": "/"
  },
  "bugs": {
    "url": "https://github.com/fleflis/cra-template-fleflis-ts/issues"
  },
  "author": {
    "name": "Fleflis",
    "url": "https://fleflis.dev"
  },
  "keywords": [
    "react",
    "create-react-app",
    "cra-template",
    "template",
    "eslint",
    "typescript",
    "prettier",
    "styled-components"
  ],
  "description": "A quick start Create React App template, with Prettier, ESLint, styled-components and Typescript",
  "homepage": "https://github.com/flefis/cra-template-fleflis-ts#readme"
}
